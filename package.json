    "@semantic-release/npm": "^8.0.3",
    "remeda": "^0.0.32",
    "semantic-release": "18.0.1",
    "semantic-release-plugin-decorators": "^3.0.1"
  },
  "devDependencies": {
    "@nrwl/cli": "13.2.3",
    "@nrwl/eslint-plugin-nx": "13.2.3",
    "@nrwl/jest": "13.2.3",
    "@nrwl/linter": "13.2.3",
    "@nrwl/node": "13.2.3",
    "@nrwl/nx-cloud": "12.5.4",
    "@nrwl/tao": "13.2.3",
    "@nrwl/workspace": "13.2.3",
    "@types/jest": "27.0.2",
    "@types/node": "^14.14.33",
    "@types/semantic-release": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "gitlog": "^4.0.4",
    "jest": "27.2.3",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "ts-node": "~9.1.1",
    "ttypescript": "^1.5.13",
    "type-fest": "^2.8.0",
    "typescript": "4.3.5"
  }
